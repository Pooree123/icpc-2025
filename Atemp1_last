#include <iostream>
#include <vector>
using namespace std;

// ฟังก์ชันสำหรับยกกำลังแบบ mod (modular exponentiation)
long long mod_pow(long long base, long long exp, long long mod) {
    long long result = 1;
    base %= mod;
    while (exp > 0) {
        if (exp & 1) result = (result * base) % mod;
        base = (base * base) % mod;
        exp >>= 1;
    }
    return result;
}

int main() {
    long long p;
    cin >> p;

    if (p >= 2 && p < 100000) {
        vector<long long> a(p, 0);

        // นับความถี่ของ i^j mod p
        for (int i = 1; i <= p - 1; i++) {
            for (int j = 1; j <= p - 1; j++) {
                long long check = mod_pow(i, j, p);
                a[check]++;
            }
        }

        // แสดงผล
        for (int i = 1; i <= p - 1; i++) {
            cout << a[i] << " ";
        }
    } else {
        cout << "0";
    }

    return 0;
}
